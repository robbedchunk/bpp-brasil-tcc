{
  "name": "E-commerce Product Details",
  "baseSelector": ".container-default",
  "fields": [
    {
      "name": "product_id",
      "selector": "p:contains('C\u00f3digo:')",
      "type": "text"
    },
    {
      "name": "product_name",
      "selector": "[data-testid='pdp-product-name']",
      "type": "text"
    },
    {
      "name": "product_description",
      "selector": "#product-description .reset-styles",
      "type": "text"
    },
    {
      "name": "brand_name",
      "selector": "p:contains('Marca:') a",
      "type": "text"
    },
    {
      "name": "package_size",
      "selector": "h2[data-testid='pdp-product-name']",
      "type": "text"
    },
    {
      "name": "category_info",
      "selector": ".flex.items-center.gap-x-2.cursor-pointer",
      "type": "text"
    },
    {
      "name": "current_price",
      "selector": ".text-blue-royal.font-bold.text-xl",
      "type": "text"
    }
  ]
}